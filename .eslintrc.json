{
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "off"
  },
  "plugins": [
    "react"
  ]
}
